{
    "homepage": "https://github.com/ueffel/Keypirinha-Plugin-Kill",
    "description": "keypirinha-plugin keypirinha-package",
    "version": "2.8",
    "url": "https://github.com/ueffel/Keypirinha-Plugin-Kill/releases/download/2.8/Kill.keypirinha-package",
    "hash": "",
    "depends": [
        "PSWinUtil",
        "keypirinha"
    ],
    "installer": {
        "script": [
            "$packageName = '{0}.keypirinha-package' -f ($app -replace '^keypirinha-', '')",
            "$packageDir = '{0}/../portable/Profile/Packages' -f (PSWinUtil\\Find-WUPath -Name 'keypirinha.exe' -Program -Strict)",
            "Move-Item -LiteralPath ('{0}/{1}' -f $dir, $packageName) -Destination $packageDir"
        ]
    },
    "uninstaller": {
        "script": [
            "$packageName = '{0}.keypirinha-package' -f ($app -replace '^keypirinha-', '')",
            "$packageDir = '{0}/../portable/Profile/Packages' -f (PSWinUtil\\Find-WUPath -Name 'keypirinha.exe' -Program -Strict)",
            "Remove-Item -LiteralPath ('{0}/{1}' -f $packageDir, $packageName)"
        ]
    },
    "bin": "",
    "shortcuts": [
        
    ],
    "checkver": "github",
    "autoupdate": {
        "url": "https://github.com/ueffel/Keypirinha-Plugin-Kill/releases/download/$version/Kill.keypirinha-package"
    }
}
